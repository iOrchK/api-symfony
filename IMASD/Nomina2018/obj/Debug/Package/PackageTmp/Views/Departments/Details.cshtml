@model Nomina2018.Models.Departamento

@{
    ViewBag.Title = "Departamento - Detalle";
}

    <h2 class="uk-margin-top">Detalle</h2>

<div>
    <h4>Departamento</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Nombre:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nombre)
        </dd>

        <dt>
            Descripción
        </dt>

        <dd>
            @Html.DisplayFor(model => model.descripcion)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Editar", "Edit", "Departments", new { id = Model.idDepto }, new { @class = "uk-button uk-button-primary" })
    @Html.ActionLink("Regresar a la lista", "Index", "Departments", null, new { @class = "uk-button uk-button-default" })
</p>
